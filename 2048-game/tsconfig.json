{
  "compilerOptions": {
    "target": "ES2015",
    "baseUrl": "./src",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext",
      "es6",
      "es2016",
      "es2017"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "noImplicitAny": false,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src"
  ]
}
